{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Clear Logs",
      "type": "shell",
      "command": "${command:python.interpreterPath}",
      "args": [
        "/Users/maxludden/dev/py/maxgradient/tasks/clear.py"
      ],
      "isBackground": true,
      "problemMatcher": {
        "owner": "python",
        "fileLocation": [
          "relative",
          "${workspaceFolder}/tasks/clear.py"
        ],
        "pattern": {
          "regexp": "^.*$",
          "file": 1,
          "location": 2,
          "message": 0
        }
      }
    }
  ]
}